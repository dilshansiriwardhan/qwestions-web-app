{
    "version": 2,
    "builds": [
      {
        "src": "index.js",
        "use": "@vercel/node"
      }
    ],
    "routes": [
      {
        "src": "/api/exams/(.*)",
        "dest": "routes/exams.routes.js"
      },
      {
        "src": "/api/questions/(.*)",
        "dest": "routes/questions.routes.js"
      }
    ]
  }
  